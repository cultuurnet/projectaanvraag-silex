{
    "name": "cultuurnet/projectaanvraag-silex",
    "description": "A silex application that serves as API backend for the projectaanvraag application",
    "license": "GPL-3.0",
    "require": {
        "cultuurnet/auth": "dev-master as 1.2.x-dev",
        "cultuurnet/calendar-summary-v3": "dev-master",
        "cultuurnet/culturefeed-http-guzzle": "dev-master",
        "cultuurnet/culturefeed-php": "dev-master as dev-develop",
        "cultuurnet/search-v3": "dev-master",
        "cweagans/composer-patches": "^1.5",
        "dflydev/doctrine-orm-service-provider": "^2.0",
        "doctrine/dbal": "~2.2",
        "doctrine/mongodb": "1.4.*",
        "doctrine/mongodb-odm": "1.1.*",
        "filp/whoops": "^2.5",
        "guzzlehttp/guzzle": "^6.3",
        "hassankhan/config": "^2.1",
        "kevinrob/guzzle-cache-middleware": "^3.2",
        "league/container": "^3.3",
        "league/route": "^4.3",
        "matthiasmullie/minify": "^1.3",
        "ml/json-ld": "^1.0",
        "mongodb/mongodb": "1.1.*",
        "predis/predis": "^1.1",
        "simple-bus/jms-serializer-bridge": "^1.0",
        "simple-bus/rabbitmq-bundle-bridge": "^3.0",
        "symfony/browser-kit": "^3.1",
        "symfony/css-selector": "3.2.*",
        "symfony/debug": "3.2.*",
        "symfony/dom-crawler": "3.2.*",
        "symfony/framework-bundle": "3.1.*",
        "symfony/inflector": "3.2.*",
        "symfony/monolog-bridge": "3.2.*",
        "symfony/property-access": "3.2.*",
        "symfony/twig-bridge": "^3.3",
        "zendframework/zend-httphandlerrunner": "^1.1"
    },
    "extra": {
        "patches": {
        }
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\ProjectAanvraag\\": ["src", "app"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\ProjectAanvraag\\": ["test"]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.7",
        "satooshi/php-coveralls": "^1.0",
        "escapestudios/symfony2-coding-standard": "^2.9",
        "phing/phing": "^2.16"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "ext-mongo": "1.6.16"
        },
        "sort-packages": true
    }
}
